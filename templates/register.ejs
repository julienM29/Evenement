<%- include('./shared/header.ejs', {title: 'Se connecter' })%>
    <h1>Cr√©ation de compte</h1>
    <div class="container">
        <div class="card mt-2 px-2">
            <form action="" method="post">
                <div class="form-group">
                    <label for="nom">Nom d'utilisateur</label>
                    <input type="text" class="form-control" name="nom" id="nom">
                </div>
                <div class="form-group">
                    <label for="prenom">Prenom d'utilisateur</label>
                    <input type="text" class="form-control" name="prenom" id="prenom">
                </div>
                <div class="form-group">
                    <label for="email">Email d'utilisateur</label>
                    <input type="email" class="form-control" name="email" id="email">
                </div>
                <div class="form-group">
                    <label for="photo">Photo d'utilisateur</label>
                    <input type="file" class="form-control" name="photo" id="photo">
                </div>
                <div class="form-group">
                    <label for="password">Mot de passe</label>
                    <input type="password" class="form-control" name="password" id="password">
                </div>
                <button class="mt-3 btn btn-primary"> Valider les informations</button>
            </form>
        </div>
    </div>
    <%- include('./shared/footer.ejs')%>